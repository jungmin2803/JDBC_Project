package day0221;

public class GouguFor_05 {

	public static void main(String[] args) {
		// 2~9단까지의 구구단
		
		for(int dan=2;dan<=9;dan++)
		{
			System.out.printf("===%d===",dan);
			for(int j=1;j<=9;j++)
			{
				System.out.printf("%dX%d=%d\n",dan,j,dan*j);
		    }
			System.out.println();
		}
		

	}

}
