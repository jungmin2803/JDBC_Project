package day0228;

class Student{
	
	private String stuName;
	private String hp;
	private int score;
	
	//명시적 생성자 인자값 3개
	
	public Student(String name, String hp, int score)
	{
		this.stuName=name;
		this.hp=hp;
		this.score=score;
	}
	
	public void writeData()
	{
		System.out.println("이름: "+stuName);
		System.out.println("핸드폰: "+hp);
		System.out.println("점수: "+score);		
	}
	
}
