package day0221;

import java.util.Scanner;

public class GuguScan_07 {

	public static void main(String[] args) {
		// 원하는 구구단 입력하시오
		//5
		//5단만 출력하시오
		
		Scanner sc=new Scanner(System.in);
		int su;
		System.out.println("원하는 구구단을 입력하시오");
		su=sc.nextInt();
		dog: for(int i=su;i<=9;i++) 
		{				
			for(int j=1;j<=9;j++)
			{ if(j==9) 
				break dog;
			
			System.out.println(i+"X"+j+"="+i*j);
			}
			System.out.println();			
		}
			
		
		
	}

}
