package day0227;

class Test01 {
	
	private String MyName;
	private static String Message;
	
	public Test01(String MyName, String Message)
	{
		this.MyName=MyName;
		this.Message=Message;				
		System.out.println("주민명: "+MyName+"\n메세지: "+Message);
	}
	
	public String getMyName()
	{
		return MyName;
	}
	
	public String getMessage()
	{
		return Message;
	}
	
}
////////////////////////////
public class Test00 {
	private String city;
	private int age;
	
	public Test00(String city, int age)
	{
		this.city=city;
		this.age=age;
		System.out.println("지역: "+city+"\n나이: "+age);
	}
	
	public String getCity()
	{
		return city;
	}
	
	public int getAge()
	{
		return age;
	}

	public static void main(String[] args) {
		/*	
		 * [개인정보]	 
		 * 주민명: 민영기
		 * 메세지: 행복하게 살자
		 * 지역: 대전
		 * 나이: 23
		 * 
		 */
		
		Test01 t1=new Test01("민영기", "행복하게 살자");
		Test00 t2=new Test00("대전", 23);
		
	}

}
