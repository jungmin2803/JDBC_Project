package day0228;



class Ex0Score{

	private String myName;
	private int java;
	private int oracle;
	private int spring;
	private int tot;
	private double avg;
	private static String mes="쌍용고 4반 중간고사";	

	public String getMyName() {
		return myName;
	}
	public void setMyName(String myName) {
		this.myName = myName;
	}
	public int getJava() {
		return java;
	}
	public void setJava(int java) {
		this.java = java;
	}
	public int getOracle() {
		return oracle;
	}
	public void setOracle(int oracle) {
		this.oracle = oracle;
	}
	public int getSpring() {
		return spring;
	}
	public void setSpring(int spring) {
		this.spring = spring;
	}
	public int getTot() {
		return tot;
	}
	public void setTot(int tot) {
		this.tot = tot;
	}
	public double getAvg() {
		return avg;
	}
	public void setAvg(double avg) {
		this.avg = avg;
	}
	public static String getMes() {
		return mes;
	}
	public static void setMes(String mes) {
		Ex0Score.mes = mes;
	}
	
	public void setScore(String name, int java, int oracle, int spring)
	{
		myName=name;
		this.java=java;
		this.oracle=oracle;
		this.spring=spring;		
	}
	
	public void setTot()
	{
		tot=java+oracle+spring;
	}
	public void setAge()
	{
		avg=tot/3.0;
	}
	


}


