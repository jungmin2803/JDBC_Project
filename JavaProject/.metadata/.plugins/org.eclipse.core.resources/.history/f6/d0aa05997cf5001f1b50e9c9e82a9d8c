package day0228;
//이론
//데이터 타입
//기본형(callbyValue): int, long, double, char, boolean... (8가지)...값이전달
//레퍼런스형: 클래스형태로 된것, 배열...주소가 전달
public class Ex5CallByValRef {
	
	//1. callbyValue
	public static void changeInt(int n) {
		System.out.println("전달받은 정수값: "+n);
		n=20;
	}
	
	//2. callby레퍼런스
	public static void changeArray(int [] arr)
	{
		System.out.println("전달받은 배열출력");
		for(int a:arr)
			System.out.println(a);
		arr[1]=80;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//1.value
		int a=50;
		changeInt(a);
		System.out.println("a값이 바뀔까?"+a); //안바뀜
		
		//2.배열
		int [] arr2= {10,20,30};//객체로 인식...레퍼런스 변수
		
		
		changeArray(arr2);
		
	}

}
