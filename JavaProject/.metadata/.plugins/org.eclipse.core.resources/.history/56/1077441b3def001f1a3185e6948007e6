package day0220;

public class ForWhileTest_08 {

	public static void main(String[] args) {
		// 10 9 8 7 ....0
		
		for(int i=10;i>=0;i--) {
			System.out.printf(i+"  ");
		}
			System.out.println();
			
		//while
		int a=10;
		while(a>=0)
		{
			System.out.printf(a--+" ");//초기값을 출력후 하나감소 10 9 8 7...0
			System.out.printf(--a+" ");//
			
		}
		
		
	}

}
