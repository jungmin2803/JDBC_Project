package day0225;

public class Test_00 {

	public static void main(String[] args) {
		//임의의 수를 로또변수에 발생시켜서 넣어준다
		
		int [] lotto= {6};
		
		for(int i=0;i<lotto.length;i++)
		{
			lotto[i]=(int)(Math.random()*45)+1;
			
			for(int j=0;j<i;j++)
			{//중복 제거
				if(lotto[i]==lotto[j])
				{
					i--;
					break;
				}
			}
		}
		//오름차순
		
		
		
		
		

	}

}
