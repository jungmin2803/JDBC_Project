package day0305;

public class Ex17StringToken {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String msg="red|pink|Yellow|black|white";
		//String mag="red,pink,Yellow,black,white";

		//|로 분리해서 출력
		String []arr=msg.split("|");
		for(String a:arr)
			System.out.print(a+"  ");
	}

}
