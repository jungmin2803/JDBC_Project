public class test {

	public static void main(String[] args) {
		/*
		 * 4개의 수를 입력하시오
		 * 5
		 * 15
		 * 8
		 * 10
		 * ***if문***
		 * max=15
		 * ***삼항연산자***
		 * max=15
		 */
		
		Scanner sc=new Scanner(System.in);
		
		int x,y,z,c,max;
		
		
		//if
		if(x>y&&x>z&&x>c)
			max=x;
		
		
		
		

	}

}
