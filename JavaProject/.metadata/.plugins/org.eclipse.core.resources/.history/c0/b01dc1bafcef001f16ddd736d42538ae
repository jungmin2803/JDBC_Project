package day0221;

import java.util.Scanner;

public class GuguScan_07 {

	public static void main(String[] args) {
		// 원하는 구구단 입력하시오
		//5
		//5단만 출력하시오
		
		Scanner sc=new Scanner(System.in);
		int su;
		
		for(int i=1;i<=9;i++) 
		{			System.out.println("원하는 구구단을 입력하시오");
					su=sc.nextInt();
			for(int j=1;j<=9;j++)
			{
				
			System.out.println(i+"X"+j+"="+i*j);
			}	System.out.println();			
		}
			
		
		
	}

}
