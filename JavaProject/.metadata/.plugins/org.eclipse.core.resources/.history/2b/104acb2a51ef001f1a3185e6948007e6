package day0220;

public class ForTest_11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		/* ex) int i=1;
		 *     for(;i<=10;i++)
		 */
		
		int i; //초기값
		for(i=1;i<=10;i++) {
			System.out.println(i+"  ");			
		}
		System.out.println("\n빠져나온후의 ㅑ값: "+i);
		
		int a=10;
		for(;a<=50;a+=5)
			System.out.println(a+"  ");
		System.out.println("\n빠져나온후의 a값: "+a);
		
	}

}
