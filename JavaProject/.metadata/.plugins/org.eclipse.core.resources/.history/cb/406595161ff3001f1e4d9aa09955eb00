package day0225;

public class ArrayIntTemp_03 {

	public static void main(String[] args) {
		// 정렬을 알기전에 순서바꾸기를 먼저 연습
		
		int a=10,b=20;
		System.out.println("a="+a+" b="+b);
		
		int temp; // temp: 임시 저장소(빈그릇) ex)a와 b의 값을 바꾸기 위해 임시로 값을 저장하는 장소 
		
		temp=a;
		a=b;
		b=temp;
		System.out.println("a="+a+" b="+b);
		
		
	}

}
