package day0305;

//super
class Car{
	private String carName;
	private String carColor;
	
	//디폴트생성자
	public Car() {
		// TODO Auto-generated constructor stub
	}
	
	//명시적생성자
	public Car(String name, String color) {
		carName=name;
		carColor=color;
	}
	//setter & getter
	public String getCarName() {
		return carName;
	}
	public void setCarName(String carName) {
		this.carName = carName;
	}
	public String getCarColor() {
		return carColor;
	}
	public void setCarColor(String carColor) {
		this.carColor = carColor;
	}
	
	//메서드
	public void writeCar() {
		System.out.println("내차는 "+carName+"이고 색상은 "+carColor+" 입니다");
	}
}

//sub
class MyCar extends Car{
	
	private int carPrice;
	
	//디폴트 생성자 //명시적 생성자 작성 안하면 디폴트 작성 안해도 생략가능
	public MyCar() {
		super(); //생략되어있음 
	}
	
	//명시적 생성자
	public MyCar(String carName,String carColor, int carPrice) {
		super(carName, carColor);
		this.carPrice=carPrice;
	}
	
	//setter & getter
	public int getCarPrice() {
		return carPrice;
	}

	public void setCarPrice(int carPrice) {
		this.carPrice = carPrice;
	}
	
	//오버라이딩
	@Override
	public void writeCar() {
		// TODO Auto-generated method stub
		super.writeCar();
		System.out.println("내차의 가격은 "+carPrice+"원 입니다");
	}
	
}

public class Ex1Extends {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//자식클래스 디폴트
		MyCar mc=new MyCar();
		mc.setCarName("카니발");
		mc.setCarColor("블랙");
		mc.setCarPrice(50000000);
		mc.writeCar();
		
		//자식클래스 명시적
		MyCar mc1=new MyCar("아우디", "화이트", 60000000);
		mc1.writeCar();
	}

}
